<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rudoxy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rudoxy">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumberActivity"
            android:label="Number"/>
        <activity android:name=".PhrasesActivity"
            android:label="Phrases"
            />
        <activity android:name=".SmsActivity"
            android:label="Sms"/>
        <activity android:name=".ColorActivity"
            android:label="Color"/>
        <activity android:name=".EmailActivity"
            android:label="Email"/>
        <activity android:name=".FamilymemberActivity"
            android:label="Family Member"/>
    </application>

</manifest>